// O(length of array)
// this change made
class Solution {
    public int maxProfit(int[] prices) {
        int costprice=Integer.MAX_VALUE;
        int maxprofit=0;
        int profit;
        for(int i=0; i<prices.length; i++){
            if(costprice<prices[i]){
                profit= prices[i]-costprice;
                maxprofit= Math.max(maxprofit,profit);  
            }
            else{
                costprice=prices[i];
            }
        }
        return maxprofit;
    }
}
